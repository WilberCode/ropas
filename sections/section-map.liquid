{% if section.settings.title != blank or section.settings.title_sub != blank or section.settings.content != blank or section.settings.image != blank or section.settings.address != blank and section.settings.api_key != blank %}
  
  {%- liquid
    assign layout = section.settings.layout | split: '-'
    assign full_width = section.settings.full_width
    if section.settings.enable_custom_colors
      assign map_background_color = section.settings.map_background_color
      assign map_foreground_color = section.settings.map_foreground_color
    endif
    assign text_color_mobile = false
    if section.settings.section_color_mobile != 'rgba(0,0,0,0)' and section.settings.section_color_mobile != blank
      assign text_color_mobile = true
      assign text_color_section_mobile = section.settings.section_color_mobile
    endif     
    assign text_color_hover_mobile = false
    if section.settings.section_color_hover_mobile != 'rgba(0,0,0,0)' and section.settings.section_color_hover_mobile != blank
      assign text_color_hover_mobile = true
      assign text_color_hover_section_mobile = section.settings.section_color_hover_mobile
    endif 
    assign line_color_mobile = false
    if section.settings.section_line_mobile != 'rgba(0,0,0,0)' and section.settings.section_line_mobile != blank
      assign line_color_mobile = true
      assign line_color_section_mobile = section.settings.section_line_mobile
    endif
    assign text_color = false
    if section.settings.section_color != 'rgba(0,0,0,0)' and section.settings.section_color != blank
      assign text_color = true
      assign text_color_section = section.settings.section_color
    endif     
    assign text_color_hover = false
    if section.settings.section_color_hover != 'rgba(0,0,0,0)' and section.settings.section_color_hover != blank
      assign text_color_hover = true
      assign text_color_hover_section = section.settings.section_color_hover
    endif 
    assign line_color = false
    if section.settings.section_line != 'rgba(0,0,0,0)' and section.settings.section_line != blank
      assign line_color = true
      assign line_color_section = section.settings.section_line
    endif
    assign text_background = false
    assign text_background_opacity = section.settings.text_background_opacity | times: 0.01
    if section.settings.text_background != 'rgba(0,0,0,0)' and section.settings.text_background != blank
      assign text_background = true
      assign text_background_section = section.settings.text_background
    endif  
    if section.settings.text_background_gradient != 'rgba(0,0,0,0)' and section.settings.text_background_gradient != blank
      assign text_background = true
      assign text_background_section = section.settings.text_background_gradient
    endif 
    assign overlay_color = false
    assign overlay_color_opacity = section.settings.overlay_opacity | times: 0.01
    if section.settings.overlay_background != 'rgba(0,0,0,0)' and section.settings.overlay_background != blank
      assign overlay_color = true
      assign overlay_color_section = section.settings.overlay_background
    endif  
    if section.settings.overlay_background_gradient != 'rgba(0,0,0,0)' and section.settings.overlay_background_gradient != blank
      assign overlay_color = true
      assign overlay_color_section = section.settings.overlay_background_gradient
    endif
    assign background_color = false
    if section.settings.background != 'rgba(0,0,0,0)' and section.settings.background != blank
      assign background_color = true
      assign background_color_section = section.settings.background
    endif  
    if section.settings.background_gradient != 'rgba(0,0,0,0)' and section.settings.background_gradient != blank
      assign background_color = true
      assign background_color_section = section.settings.background_gradient
    endif
  -%}
  
  {% style %}
    .dynamic-section-{{ section.id }} {
      {% if full_width %}
        --section_height_mobile: {{ section.settings.section_height_mobile }}vh; 
        --section_height_desktop: {{ section.settings.section_height_desktop }}vh; 
        --section_padding_mobile: 0; 
        --section_padding_desktop: 0; 
      {% else %}
        --section_height_mobile: auto; 
        --section_height_desktop: auto; 
        --section_padding_mobile: {{ section.settings.section_height_mobile }}%; 
        --section_padding_desktop: {{ section.settings.section_height_desktop }}%; 
      {% endif %}
      {%- if section.settings.image_desktop != blank -%}--image_height_desktop: {{ section.settings.image_desktop.height }}px;{%- endif -%}
      {%- if section.settings.image_mobile != blank -%}--image_height_mobile: {{ section.settings.image_mobile.height }}px;{%- endif -%}
      --content_width: {{ section.settings.content_width }}%;
      --section-padding-top-mobile: {{ section.settings.padding_top_mobile }}px;
      --section-padding-top-desktop: {{ section.settings.padding_top_desktop }}px;
      --section-padding-bottom-mobile: {{ section.settings.padding_bottom_mobile }}px;
      --section-padding-bottom-desktop: {{ section.settings.padding_bottom_desktop }}px;
      --section-margin-bottom-mobile: {{ section.settings.margin_bottom_mobile }}px;
      --section-margin-bottom-desktop: {{ section.settings.margin_bottom_desktop }}px;  
      {%- if text_color_mobile -%}
      --color_title_text:{{ text_color_section_mobile }};
      --color_text:{{ text_color_section_mobile }};
      --color_accent:{{ text_color_section_mobile }};
      --color_accent_modify_25:{{ text_color_section_mobile | color_modify: 'alpha', 0.25 }};
      {%- endif -%}
      {%- if text_color_hover_mobile -%}
      --color_text_hover:{{ text_color_hover_section_mobile }};
      {%- endif -%}
      {%- if line_color_mobile -%}
      --color_border:{{ line_color_section_mobile }};
      {%- endif -%}
      {%- if background_color -%}
      --color_background:{{ background_color_section }};
      {%- endif -%}
      {%- if overlay_color -%}
      --color_overlay:{{ overlay_color_section }};
      --color_overlay_opacity:{{ overlay_color_opacity }};
      {%- endif -%}
    }
    {%- if text_color or text_color_hover or line_color or text_background or overlay_color -%}
      @media (min-width: 640px) {
        .dynamic-section-{{ section.id }} {
          {%- if text_color -%}
          --color_title_text:{{ text_color_section }};
          --color_text:{{ text_color_section }};
          --color_accent:{{ text_color_section }};
          --color_accent_modify_25:{{ text_color_section | color_modify: 'alpha', 0.25 }};
          {%- endif -%}  
          {%- if text_color_hover -%}
          --color_text_hover:{{ text_color_hover_section }};
          {%- endif -%}
          {%- if line_color -%}
          --color_border:{{ line_color_section }};
          {%- endif -%}
          {%- if text_background -%} 
          --text_overlay:{{ text_background_section }};
          --text_overlay_opacity:{{ text_background_opacity }};
          {%- endif -%}
        }
    }
    {%- endif -%}
  {% endstyle %}
  
  <div class="dynamic-section dynamic-section-{{ section.id }}">
    <section data-section-id="{{ section.id }}" 
             data-section-type="map-{{ section.id }}"
             {% if section.settings.address != blank %}
             data-url="{{ 'script.map.js' | asset_url }}" data-google="https://maps.googleapis.com/maps/api/js?key={{ section.settings.api_key | default: false }}"
             {%  endif %}
             data-map
             data-api-key="{{ section.settings.api_key | default: false }}"
             data-address="{{ section.settings.address }}"
             data-zoom="{{ section.settings.zoom }}"
             {% if section.settings.enable_custom_colors %}
             data-map-color-a="{{ map_foreground_color }}"
             data-map-color-b="{{ map_background_color | color_mix: map_foreground_color, 30 }}"
             data-map-color-c="{{ map_background_color | color_mix: map_foreground_color, 50 }}"
             data-map-color-d="{{ map_background_color | color_mix: map_foreground_color, 70 }}"
             data-map-color-e="{{ map_background_color | color_mix: '#ffffff', 98 }}"
             data-map-color-f="{{ map_background_color | color_mix: '#000000', 50 }}"
             {% endif %}
             class="{% if full_width %}section-skip {% endif %}text-{{ section.settings.title_alignment }}">
      <div class="section-padding-top section-padding-bottom section-margin-bottom{% if section.settings.padding_border %} section-border{% endif %}">
  
        <div class="map section-layout{% if full_width %} full_width{% endif %} section-layout-style-{{ layout[0] }} section-layout-y-{{ layout[0] }}-{{ layout[1] }} section-layout-x-{{ layout[0] }}-{{ layout[2] }} section-layout-mobile-{{ section.settings.layout_mobile }}">

          {%- liquid
            if section.settings.image_desktop != blank or section.settings.image_mobile != blank
              if layout[0] == 'outside'
                if full_width
                  assign sizes = '(min-width: 3440px) 1720px, (min-width: 640px) 50vw, 100vw'
                else
                  assign size = 662
                  assign sizes = '(min-width: 1280px) 620px, (min-width: 960px) calc(50vw - 40px), (min-width: 640px) 50vw, 100vw'  
                endif
              else
                if full_width
                  assign sizes = '(min-width: 3440px) 3440px, 100vw'
                else
                  assign size = 1240
                  assign sizes = '(min-width: 1280px) 1240px, (min-width: 960px) calc(100vw - 40px), 100vw'
                endif
              endif
              if forloop.first
                if section.settings.enable_lazy
                  assign lazy = 'lazy'
                else
                  assign lazy = 'eager'
                endif
              else
                assign lazy = 'lazy'
              endif
            endif
          -%}
  
          <div class="section-layout-wrapper">

            {% if section.settings.image_desktop != blank %}  
              <figure class="{{ section.settings.insta_filter }} section-layout-wrapper image-height-desktop{% if section.settings.image_mobile != blank %} desktop{% endif %}">
                <div class="height-padding-custom desktop">
                  {%- liquid
                    assign img_src = section.settings.image_desktop
                    if full_width
                      assign size = img_src.width
                    endif
                    capture alt
                      if img_src.alt != blank
                        echo img_src.alt | escape
                      else
                        echo img_src | split: '/' | last | replace: '-', ' ' | replace: '_', ' ' | split: '.' | first | capitalize
                      endif
                    endcapture              
                    render 'image.load', img_src: img_src, size: size, new_size: size, padding: section.settings.section_height_desktop, sizes: sizes, alt: alt, background: true, lazy: lazy, full_width: full_width           
                    if layout[0] == 'outside' and section.settings.overlay_opacity != 0 and overlay_color
                      echo '<div class="section-layout-overlay"></div>'
                    endif
                  -%}
                </div>
              </figure>
            {% endif %}
              
            {% if section.settings.image_mobile != blank %}  
              <figure class="{{ section.settings.insta_filter_mobile }} section-layout-wrapper{% if section.settings.image_desktop != blank %} mobile{% endif %}">
                <div class="height-padding-custom mobile">
                  {%- liquid
                    assign img_src = section.settings.image_mobile
                    if full_width
                      assign size = img_src.width
                    endif
                    capture alt
                      if img_src.alt != blank
                        echo img_src.alt | escape
                      else
                        echo img_src | split: '/' | last | replace: '-', ' ' | replace: '_', ' ' | split: '.' | first | capitalize
                      endif
                    endcapture
                    render 'image.load', img_src: img_src, size: size, new_size: size, padding: section.settings.section_height_mobile, sizes: sizes, alt: alt, background: true, lazy: lazy, full_width: full_width
                    if layout[0] == 'outside' and section.settings.overlay_opacity != 0 and overlay_color
                      echo '<div class="section-layout-overlay"></div>'
                    endif
                  -%}
                </div>
              </figure>
            {% endif %}
            
            <div class="map-container{% if section.settings.image_desktop == blank and section.settings.image_mobile == blank %} skip{% endif %} height-padding-custom mixed" data-map-container></div>
            {%- liquid
              if section.settings.image_desktop == blank and section.settings.image_mobile == blank and layout[0] == 'outside' and section.settings.overlay_opacity != 0 and overlay_color
                echo '<div class="section-layout-overlay skip"></div>'
              endif
            -%}
            
          </div>
  
          {%- liquid
            if section.settings.image_desktop == blank and section.settings.image_mobile == blank and section.settings.address == blank and section.settings.api_key == blank
              if layout[0] == 'overlay' and section.settings.overlay_opacity != 0 and overlay_color
                echo '<div class="section-layout-overlay"></div>'
              endif
            endif
          -%}
  
          {% if section.settings.title != blank or section.settings.title_sub != blank or section.settings.content != blank %}
            <div class="{% if full_width %}section-skip-container {% endif %}section-layout-card-wrapper">
              
              <div class="section-layout-card">
            
                {% liquid
                  if section.settings.text_background != 0 and text_background
                    echo '<div class="section-layout-card-overlay"></div>'
                  endif
                -%}
  
                {% if section.settings.title != blank %}
                  {%- liquid
                    if section.settings.title_h1
                      assign title = 1
                    else
                      assign title = 2
                    endif
                  -%}
                  <h{{ title }} class="section-title{% if section.settings.title_sub != blank %} section-title-half{% endif %} section-title-{{ section.settings.title_size }} {{ section.settings.title_outline }} no-pad">{{ section.settings.title }}</h{{ title }}>
                {% endif %}
  
                {% if section.settings.title_sub != blank %}
                  <h3 class="section-title section-title-{{ section.settings.title_sub_size }} {{ section.settings.title_sub_outline }} no-pad">{{ section.settings.title_sub }}</h3>  
                {% endif %}        
  
                {% if section.settings.content != blank %}
                  <div class="section-color{% if section.settings.text_border %} text-border{% endif %} section-layout-card-content">{{ section.settings.content }}</div>
                {% endif %}
  
              </div>
            </div>
          {% endif %}
  
        </div>
      </div>
    </section>
  </div>

{% endif %}

